<%= search_form_for @q, :class => collapse_search_form, :id => "slots_filters" do |f| %>
  <p class="lead">Narrow results:</p>

  <div class="form-group">
    <%= f.label :end_time, "End time" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='end_timegteqPicker' data-target-input="nearest">
          <%= f.text_field :end_time_gteq, :class => "form-control datetimepicker-input", :placeholder => "End time greater than or equal to", :data => {:target => '#end_timegteqPicker'} %>
          <div class="input-group-append" data-target='#end_timegteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-clock-o"></i></div>
          </div>
          <script type="text/javascript">
            $(function () {
                $('#end_timegteqPicker').datetimepicker({
                  format: 'LT'
                });
            });
          </script>
        </div>
      </div>
      <div class='col-md-6'>
        <div class='input-group date' id='end_timelteqPicker' data-target-input="nearest">
          <%= f.text_field :end_time_lteq, :class => "form-control datetimepicker-input", :placeholder => "End time less than or equal to", :data => {:target => '#end_timelteqPicker'} %>
          <div class="input-group-append" data-target='#end_timelteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-clock-o"></i></div>
          </div>
          <script type="text/javascript">
            $(function () {
                $('#end_timelteqPicker').datetimepicker({
                  format: 'LT'
                });
            });
          </script>
        </div>
      </div>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :start_time, "Start time" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='start_timegteqPicker' data-target-input="nearest">
          <%= f.text_field :start_time_gteq, :class => "form-control datetimepicker-input", :placeholder => "Start time greater than or equal to", :data => {:target => '#start_timegteqPicker'} %>
          <div class="input-group-append" data-target='#start_timegteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-clock-o"></i></div>
          </div>
          <script type="text/javascript">
            $(function () {
                $('#start_timegteqPicker').datetimepicker({
                  format: 'LT'
                });
            });
          </script>
        </div>
      </div>
      <div class='col-md-6'>
        <div class='input-group date' id='start_timelteqPicker' data-target-input="nearest">
          <%= f.text_field :start_time_lteq, :class => "form-control datetimepicker-input", :placeholder => "Start time less than or equal to", :data => {:target => '#start_timelteqPicker'} %>
          <div class="input-group-append" data-target='#start_timelteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-clock-o"></i></div>
          </div>
          <script type="text/javascript">
            $(function () {
                $('#start_timelteqPicker').datetimepicker({
                  format: 'LT'
                });
            });
          </script>
        </div>
      </div>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :date, "Date" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='dategteqPicker' data-target-input="nearest">
          <%= f.text_field :date_gteq, :class => "form-control datetimepicker-input", :placeholder => "Date greater than or equal to", :data => {:target => '#dategteqPicker'} %>
          <div class="input-group-append" data-target='#dategteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>

      <div class='col-md-6'>
        <div class='input-group date' id='datelteqPicker' data-target-input="nearest">
          <%= f.text_field :date_lteq, :class => "form-control datetimepicker-input", :placeholder => "Date less than or equal to", :data => {:target => '#datelteqPicker'} %>
          <div class="input-group-append" data-target='#datelteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script type="text/javascript">
      $(function () {
          $('#dategteqPicker').datetimepicker({
              format: 'MM/DD/YYYY'
          });
          $('#datelteqPicker').datetimepicker({
              useCurrent: false,
              format: 'MM/DD/YYYY'
          });
          $('#dategteqPicker').on("dp.change", function (e) {
              $('#datelteqPicker').data("DateTimePicker").minDate(e.date);
          });
          $('#datelteqPicker').on("dp.change", function (e) {
              $('#dategteqPicker').data("DateTimePicker").maxDate(e.date);
          });
      });
  </script>

  <div class="form-group">
    <%= f.label :created_at, "Created at" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='created_atgteqPicker' data-target-input="nearest">
          <%= f.text_field :created_at_gteq, :class => "form-control datetimepicker-input", :placeholder => "Created at greater than or equal to", :data => {:target => '#created_atgteqPicker'} %>
          <div class="input-group-append" data-target='#created_atgteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>

      <div class='col-md-6'>
        <div class='input-group date' id='created_atlteqPicker' data-target-input="nearest">
          <%= f.text_field :created_at_lteq, :class => "form-control datetimepicker-input", :placeholder => "Created at less than or equal to", :data => {:target => '#created_atlteqPicker'} %>
          <div class="input-group-append" data-target='#created_atlteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script type="text/javascript">
      $(function () {
          $('#created_atgteqPicker').datetimepicker({
              format: 'YYYY-MM-DD hh:mm:SS'
          });
          $('#created_atlteqPicker').datetimepicker({
              useCurrent: false,
              format: 'YYYY-MM-DD hh:mm:SS'
          });
          $('#created_atgteqPicker').on("dp.change", function (e) {
              $('#created_atlteqPicker').data("DateTimePicker").minDate(e.date);
          });
          $('#created_atlteqPicker').on("dp.change", function (e) {
              $('#created_atgteqPicker').data("DateTimePicker").maxDate(e.date);
          });
      });
  </script>


  <div class="form-group">
    <%= f.label :user_email_cont, "User email contains" %>
    <%= f.text_field :user_email_cont, :class => "form-control", :placeholder => "User email contains" %>
  </div>

  <div class="form-group">
    <%= f.label :date, "Match Date" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='sent_matchesgteqdatePicker' data-target-input="nearest">
          <%= f.text_field :sent_matches_date_gteq, :class => "form-control datetimepicker-input", :placeholder => "Match Date greater than or equal to", :data => {:target => '#sent_matchesgteqdatePicker'} %>
          <div class="input-group-append" data-target='#sent_matchesgteqdatePicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>

      <div class='col-md-6'>
        <div class='input-group date' id='sent_matcheslteqdatePicker' data-target-input="nearest">
          <%= f.text_field :sent_matches_date_lteq, :class => "form-control datetimepicker-input", :placeholder => "Match Date less than or equal to", :data => {:target => '#sent_matcheslteqdatePicker'} %>
          <div class="input-group-append" data-target='#sent_matcheslteqdatePicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script type="text/javascript">
      $(function () {
          $('#sent_matchesgteqdatePicker').datetimepicker({
              format: 'MM/DD/YYYY'
          });
          $('#sent_matcheslteqdatePicker').datetimepicker({
              useCurrent: false,
              format: 'MM/DD/YYYY'
          });
          $('#sent_matchesgteqdatePicker').on("dp.change", function (e) {
              $('#sent_matcheslteqdatePicker').data("DateTimePicker").minDate(e.date);
          });
          $('#sent_matcheslteqdatePicker').on("dp.change", function (e) {
              $('#sent_matchesgteqdatePicker').data("DateTimePicker").maxDate(e.date);
          });
      });
  </script>

  <div class="form-group">
    <%= f.label :date, "Match Date" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='recipient_matchesgteqdatePicker' data-target-input="nearest">
          <%= f.text_field :recipient_matches_date_gteq, :class => "form-control datetimepicker-input", :placeholder => "Match Date greater than or equal to", :data => {:target => '#recipient_matchesgteqdatePicker'} %>
          <div class="input-group-append" data-target='#recipient_matchesgteqdatePicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>

      <div class='col-md-6'>
        <div class='input-group date' id='recipient_matcheslteqdatePicker' data-target-input="nearest">
          <%= f.text_field :recipient_matches_date_lteq, :class => "form-control datetimepicker-input", :placeholder => "Match Date less than or equal to", :data => {:target => '#recipient_matcheslteqdatePicker'} %>
          <div class="input-group-append" data-target='#recipient_matcheslteqdatePicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script type="text/javascript">
      $(function () {
          $('#recipient_matchesgteqdatePicker').datetimepicker({
              format: 'MM/DD/YYYY'
          });
          $('#recipient_matcheslteqdatePicker').datetimepicker({
              useCurrent: false,
              format: 'MM/DD/YYYY'
          });
          $('#recipient_matchesgteqdatePicker').on("dp.change", function (e) {
              $('#recipient_matcheslteqdatePicker').data("DateTimePicker").minDate(e.date);
          });
          $('#recipient_matcheslteqdatePicker').on("dp.change", function (e) {
              $('#recipient_matchesgteqdatePicker').data("DateTimePicker").maxDate(e.date);
          });
      });
  </script>


  <%= f.submit :class => "btn btn-primary btn-block mb-1" %>

  <a href="/slots" class="btn btn-outline-secondary btn-block mb-1">Clear filters</a>

<% end %>

<a class="btn btn-outline-secondary btn-block mb-1" data-toggle="collapse" href="#slots_filters">
  Show filters
</a>
